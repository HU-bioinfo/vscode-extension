{
  "name": "work-env",
  "displayName": "work-env",
  "publisher": "hu-bioinfo-workshop",
  "description": "Work environment for R/Python analysis and development",
  "repository": "https://github.com/HU-bioinfo/work-env-extension.git",
  "version": "1.1.8",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "extensionPack": [],
  "keywords": [
    "docker",
    "container",
    "r",
    "python",
    "data-science",
    "development-environment"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "work-env.start-work-env",
        "title": "work-env: Start work-env"
      },
      {
        "command": "work-env.reset-config",
        "title": "work-env: Reset work-env Config"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "npm run unit-test",
    "unit-test": "cross-env NODE_ENV=test VSCODE_MOCK=1 mocha --require ts-node/register --ui bdd --extension ts,tsx --recursive 'test/unit/**/*.test.ts' --timeout 60000",
    "test:coverage": "cross-env NODE_ENV=test VSCODE_MOCK=1 nyc npm run unit-test",
    "test:clean": "rimraf .vscode-test && rimraf test-resources",
    "test:unit": "cross-env NODE_ENV=test VSCODE_MOCK=1 mocha --require ts-node/register --ui bdd --extension ts,tsx --timeout 60000 'test/unit/**/*.test.ts'",
    "test:save-results": "scripts/save-test-results.sh",
    "preversion": "npm run test:save-results"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "chai": "^5.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "mocha": "^11.1.0",
    "mocha-sinon": "^2.1.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^20.0.0",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "dependencies": {
    "glob": "^11.0.1"
  }
}
