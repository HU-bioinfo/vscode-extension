# サンプル解析スクリプト

# 関数の読み込み
source("../R/functions.R")

# データの読み込み
# data <- read_data("../data/sample_data.csv")

# ダミーデータの作成
data <- data.frame(
  x = 1:10,
  y = rnorm(10)
)

# データの解析
result <- sample_function(data$x)

# 結果の表示
print(result)

# 結果の保存
write.csv(data.frame(x = data$x, result = result), "../results/analysis_result.csv")

# プロット作成
pdf("../results/plot.pdf")
plot(data$x, result, main = "サンプル解析", xlab = "X", ylab = "Result")
dev.off()

cat("解析が完了しました！\n") 