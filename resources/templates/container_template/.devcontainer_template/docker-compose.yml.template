services:
  container:
    image: hubioinfows/base_env:latest
    environment:
      - DISABLE_AUTH=true
      - CACHE_DIR=/home/user/cache
      - PROJ_DIR=/home/user/proj
    volumes:
      - ../projects:/home/user/proj
      - ../../cache:/home/user/cache
    env_file:
      - ../../.env
    command: sleep infinity